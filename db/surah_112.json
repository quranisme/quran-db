{"code":200,"status":"OK","data":[{"number":112,"name":"سورة الإخلاص","englishName":"Al-Ikhlaas","englishNameTranslation":"Sincerity","revelationType":"Meccan","numberOfAyahs":4,"ayahs":[{"number":6222,"audio":"http://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6222","text":"بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ قُلْ هُوَ اللَّهُ أَحَدٌ","numberInSurah":1,"juz":30,"manzil":7,"page":604,"ruku":554,"hizbQuarter":240,"sajda":false},{"number":6223,"audio":"http://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6223","text":"اللَّهُ الصَّمَدُ","numberInSurah":2,"juz":30,"manzil":7,"page":604,"ruku":554,"hizbQuarter":240,"sajda":false},{"number":6224,"audio":"http://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6224","text":"لَمْ يَلِدْ وَلَمْ يُولَدْ","numberInSurah":3,"juz":30,"manzil":7,"page":604,"ruku":554,"hizbQuarter":240,"sajda":false},{"number":6225,"audio":"http://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6225","text":"وَلَمْ يَكُنْ لَهُ كُفُوًا أَحَدٌ","numberInSurah":4,"juz":30,"manzil":7,"page":604,"ruku":554,"hizbQuarter":240,"sajda":false}],"edition":{"identifier":"ar.alafasy","language":"ar","name":"Alafasy","englishName":"Alafasy","format":"audio","type":"versebyverse"}},{"number":112,"name":"سورة الإخلاص","englishName":"Al-Ikhlaas","englishNameTranslation":"Sincerity","revelationType":"Meccan","numberOfAyahs":4,"ayahs":[{"number":6222,"text":"بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ قُلْ هُوَ ٱللَّهُ أَحَدٌ","numberInSurah":1,"juz":30,"manzil":7,"page":604,"ruku":554,"hizbQuarter":240,"sajda":false},{"number":6223,"text":"ٱللَّهُ ٱلصَّمَدُ","numberInSurah":2,"juz":30,"manzil":7,"page":604,"ruku":554,"hizbQuarter":240,"sajda":false},{"number":6224,"text":"لَمْ يَلِدْ وَلَمْ يُولَدْ","numberInSurah":3,"juz":30,"manzil":7,"page":604,"ruku":554,"hizbQuarter":240,"sajda":false},{"number":6225,"text":"وَلَمْ يَكُن لَّهُۥ كُفُوًا أَحَدٌۢ","numberInSurah":4,"juz":30,"manzil":7,"page":604,"ruku":554,"hizbQuarter":240,"sajda":false}],"edition":{"identifier":"quran-uthmani","language":"ar","name":"Uthamani","englishName":"Uthamani","format":"text","type":"quran"}},{"number":112,"name":"سورة الإخلاص","englishName":"Al-Ikhlaas","englishNameTranslation":"Sincerity","revelationType":"Meccan","numberOfAyahs":4,"ayahs":[{"number":6222,"text":"Katakanlah: \"Dialah Allah, Yang Maha Esa.","numberInSurah":1,"juz":30,"manzil":7,"page":604,"ruku":554,"hizbQuarter":240,"sajda":false},{"number":6223,"text":"Allah adalah Tuhan yang bergantung kepada-Nya segala sesuatu.","numberInSurah":2,"juz":30,"manzil":7,"page":604,"ruku":554,"hizbQuarter":240,"sajda":false},{"number":6224,"text":"Dia tiada beranak dan tidak pula diperanakkan,","numberInSurah":3,"juz":30,"manzil":7,"page":604,"ruku":554,"hizbQuarter":240,"sajda":false},{"number":6225,"text":"dan tidak ada seorangpun yang setara dengan Dia\".","numberInSurah":4,"juz":30,"manzil":7,"page":604,"ruku":554,"hizbQuarter":240,"sajda":false}],"edition":{"identifier":"id.indonesian","language":"id","name":"Bahasa Indonesia","englishName":"Unknown","format":"text","type":"translation"}},{"number":112,"name":"سورة الإخلاص","englishName":"Al-Ikhlaas","englishNameTranslation":"Sincerity","revelationType":"Meccan","numberOfAyahs":4,"ayahs":[{"number":6222,"text":"Say, \"He is Allah, [who is] One,","numberInSurah":1,"juz":30,"manzil":7,"page":604,"ruku":554,"hizbQuarter":240,"sajda":false},{"number":6223,"text":"Allah, the Eternal Refuge.","numberInSurah":2,"juz":30,"manzil":7,"page":604,"ruku":554,"hizbQuarter":240,"sajda":false},{"number":6224,"text":"He neither begets nor is born,","numberInSurah":3,"juz":30,"manzil":7,"page":604,"ruku":554,"hizbQuarter":240,"sajda":false},{"number":6225,"text":"Nor is there to Him any equivalent.\"","numberInSurah":4,"juz":30,"manzil":7,"page":604,"ruku":554,"hizbQuarter":240,"sajda":false}],"edition":{"identifier":"en.sahih","language":"en","name":"Saheeh International","englishName":"Saheeh International","format":"text","type":"translation"}}]}