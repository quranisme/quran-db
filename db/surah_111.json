{"code":200,"status":"OK","data":[{"number":111,"name":"سورة المسد","englishName":"Al-Masad","englishNameTranslation":"The Palm Fibre","revelationType":"Meccan","numberOfAyahs":5,"ayahs":[{"number":6217,"audio":"http://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6217","text":"بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ تَبَّتْ يَدَا أَبِي لَهَبٍ وَتَبَّ","numberInSurah":1,"juz":30,"manzil":7,"page":603,"ruku":553,"hizbQuarter":240,"sajda":false},{"number":6218,"audio":"http://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6218","text":"مَا أَغْنَىٰ عَنْهُ مَالُهُ وَمَا كَسَبَ","numberInSurah":2,"juz":30,"manzil":7,"page":603,"ruku":553,"hizbQuarter":240,"sajda":false},{"number":6219,"audio":"http://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6219","text":"سَيَصْلَىٰ نَارًا ذَاتَ لَهَبٍ","numberInSurah":3,"juz":30,"manzil":7,"page":603,"ruku":553,"hizbQuarter":240,"sajda":false},{"number":6220,"audio":"http://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6220","text":"وَامْرَأَتُهُ حَمَّالَةَ الْحَطَبِ","numberInSurah":4,"juz":30,"manzil":7,"page":603,"ruku":553,"hizbQuarter":240,"sajda":false},{"number":6221,"audio":"http://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6221","text":"فِي جِيدِهَا حَبْلٌ مِنْ مَسَدٍ","numberInSurah":5,"juz":30,"manzil":7,"page":603,"ruku":553,"hizbQuarter":240,"sajda":false}],"edition":{"identifier":"ar.alafasy","language":"ar","name":"Alafasy","englishName":"Alafasy","format":"audio","type":"versebyverse"}},{"number":111,"name":"سورة المسد","englishName":"Al-Masad","englishNameTranslation":"The Palm Fibre","revelationType":"Meccan","numberOfAyahs":5,"ayahs":[{"number":6217,"text":"بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ تَبَّتْ يَدَآ أَبِى لَهَبٍۢ وَتَبَّ","numberInSurah":1,"juz":30,"manzil":7,"page":603,"ruku":553,"hizbQuarter":240,"sajda":false},{"number":6218,"text":"مَآ أَغْنَىٰ عَنْهُ مَالُهُۥ وَمَا كَسَبَ","numberInSurah":2,"juz":30,"manzil":7,"page":603,"ruku":553,"hizbQuarter":240,"sajda":false},{"number":6219,"text":"سَيَصْلَىٰ نَارًۭا ذَاتَ لَهَبٍۢ","numberInSurah":3,"juz":30,"manzil":7,"page":603,"ruku":553,"hizbQuarter":240,"sajda":false},{"number":6220,"text":"وَٱمْرَأَتُهُۥ حَمَّالَةَ ٱلْحَطَبِ","numberInSurah":4,"juz":30,"manzil":7,"page":603,"ruku":553,"hizbQuarter":240,"sajda":false},{"number":6221,"text":"فِى جِيدِهَا حَبْلٌۭ مِّن مَّسَدٍۭ","numberInSurah":5,"juz":30,"manzil":7,"page":603,"ruku":553,"hizbQuarter":240,"sajda":false}],"edition":{"identifier":"quran-uthmani","language":"ar","name":"Uthamani","englishName":"Uthamani","format":"text","type":"quran"}},{"number":111,"name":"سورة المسد","englishName":"Al-Masad","englishNameTranslation":"The Palm Fibre","revelationType":"Meccan","numberOfAyahs":5,"ayahs":[{"number":6217,"text":"Binasalah kedua tangan Abu Lahab dan sesungguhnya dia akan binasa.","numberInSurah":1,"juz":30,"manzil":7,"page":603,"ruku":553,"hizbQuarter":240,"sajda":false},{"number":6218,"text":"Tidaklah berfaedah kepadanya harta bendanya dan apa yang ia usahakan.","numberInSurah":2,"juz":30,"manzil":7,"page":603,"ruku":553,"hizbQuarter":240,"sajda":false},{"number":6219,"text":"Kelak dia akan masuk ke dalam api yang bergejolak.","numberInSurah":3,"juz":30,"manzil":7,"page":603,"ruku":553,"hizbQuarter":240,"sajda":false},{"number":6220,"text":"Dan (begitu pula) istrinya, pembawa kayu bakar.","numberInSurah":4,"juz":30,"manzil":7,"page":603,"ruku":553,"hizbQuarter":240,"sajda":false},{"number":6221,"text":"Yang di lehernya ada tali dari sabut.","numberInSurah":5,"juz":30,"manzil":7,"page":603,"ruku":553,"hizbQuarter":240,"sajda":false}],"edition":{"identifier":"id.indonesian","language":"id","name":"Bahasa Indonesia","englishName":"Unknown","format":"text","type":"translation"}},{"number":111,"name":"سورة المسد","englishName":"Al-Masad","englishNameTranslation":"The Palm Fibre","revelationType":"Meccan","numberOfAyahs":5,"ayahs":[{"number":6217,"text":"May the hands of Abu Lahab be ruined, and ruined is he.","numberInSurah":1,"juz":30,"manzil":7,"page":603,"ruku":553,"hizbQuarter":240,"sajda":false},{"number":6218,"text":"His wealth will not avail him or that which he gained.","numberInSurah":2,"juz":30,"manzil":7,"page":603,"ruku":553,"hizbQuarter":240,"sajda":false},{"number":6219,"text":"He will [enter to] burn in a Fire of [blazing] flame","numberInSurah":3,"juz":30,"manzil":7,"page":603,"ruku":553,"hizbQuarter":240,"sajda":false},{"number":6220,"text":"And his wife [as well] - the carrier of firewood.","numberInSurah":4,"juz":30,"manzil":7,"page":603,"ruku":553,"hizbQuarter":240,"sajda":false},{"number":6221,"text":"Around her neck is a rope of [twisted] fiber.","numberInSurah":5,"juz":30,"manzil":7,"page":603,"ruku":553,"hizbQuarter":240,"sajda":false}],"edition":{"identifier":"en.sahih","language":"en","name":"Saheeh International","englishName":"Saheeh International","format":"text","type":"translation"}}]}