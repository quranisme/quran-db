:30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5790,"text":"مَّتَٰعًۭا لَّكُمْ وَلِأَنْعَٰمِكُمْ","numberInSurah":32,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5791,"text":"فَإِذَا جَآءَتِ ٱلصَّآخَّةُ","numberInSurah":33,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5792,"text":"يَوْمَ يَفِرُّ ٱلْمَرْءُ مِنْ أَخِيهِ","numberInSurah":34,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5793,"text":"وَأُمِّهِۦ وَأَبِيهِ","numberInSurah":35,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5794,"text":"وَصَٰحِبَتِهِۦ وَبَنِيهِ","numberInSurah":36,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5795,"text":"لِكُلِّ ٱمْرِئٍۢ مِّنْهُمْ يَوْمَئِذٍۢ شَأْنٌۭ يُغْنِيهِ","numberInSurah":37,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5796,"text":"وُجُوهٌۭ يَوْمَئِذٍۢ مُّسْفِرَةٌۭ","numberInSurah":38,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5797,"text":"ضَاحِكَةٌۭ مُّسْتَبْشِرَةٌۭ","numberInSurah":39,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5798,"text":"وَوُجُوهٌۭ يَوْمَئِذٍ عَلَيْهَا غَبَرَةٌۭ","numberInSurah":40,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5799,"text":"تَرْهَقُهَا قَتَرَةٌ","numberInSurah":41,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5800,"text":"أُو۟لَٰٓئِكَ هُمُ ٱلْكَفَرَةُ ٱلْفَجَرَةُ","numberInSurah":42,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false}],"edition":{"identifier":"quran-uthmani","language":"ar","name":"Uthamani","englishName":"Uthamani","format":"text","type":"quran"}},{"number":80,"name":"سورة عبس","englishName":"Abasa","englishNameTranslation":"He frowned","revelationType":"Meccan","numberOfAyahs":42,"ayahs":[{"number":5759,"text":"Dia (Muhammad) bermuka masam dan berpaling,","numberInSurah":1,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5760,"text":"karena telah datang seorang buta kepadanya.","numberInSurah":2,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5761,"text":"Tahukah kamu barangkali ia ingin membersihkan dirinya (dari dosa),","numberInSurah":3,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5762,"text":"atau dia (ingin) mendapatkan pengajaran, lalu pengajaran itu memberi manfaat kepadanya?","numberInSurah":4,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5763,"text":"Adapun orang yang merasa dirinya serba cukup,","numberInSurah":5,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5764,"text":"maka kamu melayaninya.","numberInSurah":6,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5765,"text":"Padahal tidak ada (celaan) atasmu kalau dia tidak membersihkan diri (beriman).","numberInSurah":7,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5766,"text":"Dan adapun orang yang datang kepadamu dengan bersegera (untuk mendapatkan pengajaran),","numberInSurah":8,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5767,"text":"sedang ia takut kepada (Allah),","numberInSurah":9,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5768,"text":"maka kamu mengabaikannya.","numberInSurah":10,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5769,"text":"Sekali-kali jangan (demikian)! Sesungguhnya ajaran-ajaran Tuhan itu adalah suatu peringatan,","numberInSurah":11,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5770,"text":"maka barangsiapa yang menghendaki, tentulah ia memperhatikannya,","numberInSurah":12,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5771,"text":"di dalam kitab-kitab yang dimuliakan,","numberInSurah":13,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5772,"text":"yang ditinggikan lagi disucikan,","numberInSurah":14,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5773,"text":"di tangan para penulis (malaikat),","numberInSurah":15,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5774,"text":"yang mulia lagi berbakti.","numberInSurah":16,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5775,"text":"Binasalah manusia; alangkah amat sangat kekafirannya?","numberInSurah":17,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5776,"text":"Dari apakah Allah menciptakannya?","numberInSurah":18,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5777,"text":"Dari setetes mani, Allah menciptakannya lalu menentukannya.","numberInSurah":19,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5778,"text":"Kemudian Dia memudahkan jalannya.","numberInSurah":20,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5779,"text":"kemudian Dia mematikannya dan memasukkannya ke dalam kubur,","numberInSurah":21,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5780,"text":"kemudian bila Dia menghendaki, Dia membangkitkannya kembali.","numberInSurah":22,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5781,"text":"Sekali-kali jangan; manusia itu belum melaksanakan apa yang diperintahkan Allah kepadanya,","numberInSurah":23,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5782,"text":"maka hendaklah manusia itu memperhatikan makanannya.","numberInSurah":24,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5783,"text":"Sesungguhnya Kami benar-benar telah mencurahkan air (dari langit),","numberInSurah":25,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5784,"text":"kemudian Kami belah bumi dengan sebaik-baiknya,","numberInSurah":26,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5785,"text":"lalu Kami tumbuhkan biji-bijian di bumi itu,","numberInSurah":27,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5786,"text":"anggur dan sayur-sayuran,","numberInSurah":28,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5787,"text":"zaitun dan kurma,","numberInSurah":29,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5788,"text":"kebun-kebun (yang) lebat,","numberInSurah":30,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5789,"text":"dan buah-buahan serta rumput-rumputan,","numberInSurah":31,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5790,"text":"untuk kesenanganmu dan untuk binatang-binatang ternakmu.","numberInSurah":32,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5791,"text":"Dan apabila datang suara yang memekakkan (tiupan sangkakala yang kedua),","numberInSurah":33,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5792,"text":"pada hari ketika manusia lari dari saudaranya,","numberInSurah":34,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5793,"text":"dari ibu dan bapaknya,","numberInSurah":35,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5794,"text":"dari istri dan anak-anaknya.","numberInSurah":36,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5795,"text":"Setiap orang dari mereka pada hari itu mempunyai urusan yang cukup menyibukkannya.","numberInSurah":37,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5796,"text":"Banyak muka pada hari itu berseri-seri,","numberInSurah":38,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5797,"text":"tertawa dan bergembira ria,","numberInSurah":39,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5798,"text":"dan banyak (pula) muka pada hari itu tertutup debu,","numberInSurah":40,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5799,"text":"dan ditutup lagi oleh kegelapan.","numberInSurah":41,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5800,"text":"Mereka itulah orang-orang kafir lagi durhaka.","numberInSurah":42,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false}],"edition":{"identifier":"id.indonesian","language":"id","name":"Bahasa Indonesia","englishName":"Unknown","format":"text","type":"translation"}},{"number":80,"name":"سورة عبس","englishName":"Abasa","englishNameTranslation":"He frowned","revelationType":"Meccan","numberOfAyahs":42,"ayahs":[{"number":5759,"text":"The Prophet frowned and turned away","numberInSurah":1,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5760,"text":"Because there came to him the blind man, [interrupting].","numberInSurah":2,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5761,"text":"But what would make you perceive, [O Muhammad], that perhaps he might be purified","numberInSurah":3,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5762,"text":"Or be reminded and the remembrance would benefit him?","numberInSurah":4,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5763,"text":"As for he who thinks himself without need,","numberInSurah":5,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5764,"text":"To him you give attention.","numberInSurah":6,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5765,"text":"And not upon you [is any blame] if he will not be purified.","numberInSurah":7,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5766,"text":"But as for he who came to you striving [for knowledge]","numberInSurah":8,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5767,"text":"While he fears [Allah],","numberInSurah":9,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5768,"text":"From him you are distracted.","numberInSurah":10,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5769,"text":"No! Indeed, these verses are a reminder;","numberInSurah":11,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5770,"text":"So whoever wills may remember it.","numberInSurah":12,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5771,"text":"[It is recorded] in honored sheets,","numberInSurah":13,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5772,"text":"Exalted and purified,","numberInSurah":14,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5773,"text":"[Carried] by the hands of messenger-angels,","numberInSurah":15,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5774,"text":"Noble and dutiful.","numberInSurah":16,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5775,"text":"Cursed is man; how disbelieving is he.","numberInSurah":17,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5776,"text":"From what substance did He create him?","numberInSurah":18,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5777,"text":"From a sperm-drop He created him and destined for him;","numberInSurah":19,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5778,"text":"Then He eased the way for him;","numberInSurah":20,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5779,"text":"Then He causes his death and provides a grave for him.","numberInSurah":21,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5780,"text":"Then when He wills, He will resurrect him.","numberInSurah":22,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5781,"text":"No! Man has not yet accomplished what He commanded him.","numberInSurah":23,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5782,"text":"Then let mankind look at his food -","numberInSurah":24,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5783,"text":"How We poured down water in torrents,","numberInSurah":25,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5784,"text":"Then We broke open the earth, splitting [it with sprouts],","numberInSurah":26,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5785,"text":"And caused to grow within it grain","numberInSurah":27,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5786,"text":"And grapes and herbage","numberInSurah":28,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5787,"text":"And olive and palm trees","numberInSurah":29,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5788,"text":"And gardens of dense shrubbery","numberInSurah":30,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5789,"text":"And fruit and grass -","numberInSurah":31,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5790,"text":"[As] enjoyment for you and your grazing livestock.","numberInSurah":32,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5791,"text":"But when there comes the Deafening Blast","numberInSurah":33,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5792,"text":"On the Day a man will flee from his brother","numberInSurah":34,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5793,"text":"And his mother and his father","numberInSurah":35,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5794,"text":"And his wife and his children,","numberInSurah":36,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5795,"text":"For every man, that Day, will be a matter adequate for him.","numberInSurah":37,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5796,"text":"[Some] faces, that Day, will be bright -","numberInSurah":38,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5797,"text":"Laughing, rejoicing at good news.","numberInSurah":39,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5798,"text":"And [other] faces, that Day, will have upon them dust.","numberInSurah":40,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5799,"text":"Blackness will cover them.","numberInSurah":41,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false},{"number":5800,"text":"Those are the disbelievers, the wicked ones.","numberInSurah":42,"juz":30,"manzil":7,"page":585,"ruku":522,"hizbQuarter":234,"sajda":false}],"edition":{"identifier":"en.sahih","language":"en","name":"Saheeh International","englishName":"Saheeh International","format":"text","type":"translation"}}]}