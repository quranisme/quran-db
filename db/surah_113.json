{"code":200,"status":"OK","data":[{"number":113,"name":"سورة الفلق","englishName":"Al-Falaq","englishNameTranslation":"The Dawn","revelationType":"Meccan","numberOfAyahs":5,"ayahs":[{"number":6226,"audio":"http://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6226","text":"بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ قُلْ أَعُوذُ بِرَبِّ الْفَلَقِ","numberInSurah":1,"juz":30,"manzil":7,"page":604,"ruku":555,"hizbQuarter":240,"sajda":false},{"number":6227,"audio":"http://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6227","text":"مِنْ شَرِّ مَا خَلَقَ","numberInSurah":2,"juz":30,"manzil":7,"page":604,"ruku":555,"hizbQuarter":240,"sajda":false},{"number":6228,"audio":"http://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6228","text":"وَمِنْ شَرِّ غَاسِقٍ إِذَا وَقَبَ","numberInSurah":3,"juz":30,"manzil":7,"page":604,"ruku":555,"hizbQuarter":240,"sajda":false},{"number":6229,"audio":"http://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6229","text":"وَمِنْ شَرِّ النَّفَّاثَاتِ فِي الْعُقَدِ","numberInSurah":4,"juz":30,"manzil":7,"page":604,"ruku":555,"hizbQuarter":240,"sajda":false},{"number":6230,"audio":"http://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6230","text":"وَمِنْ شَرِّ حَاسِدٍ إِذَا حَسَدَ","numberInSurah":5,"juz":30,"manzil":7,"page":604,"ruku":555,"hizbQuarter":240,"sajda":false}],"edition":{"identifier":"ar.alafasy","language":"ar","name":"Alafasy","englishName":"Alafasy","format":"audio","type":"versebyverse"}},{"number":113,"name":"سورة الفلق","englishName":"Al-Falaq","englishNameTranslation":"The Dawn","revelationType":"Meccan","numberOfAyahs":5,"ayahs":[{"number":6226,"text":"بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ قُلْ أَعُوذُ بِرَبِّ ٱلْفَلَقِ","numberInSurah":1,"juz":30,"manzil":7,"page":604,"ruku":555,"hizbQuarter":240,"sajda":false},{"number":6227,"text":"مِن شَرِّ مَا خَلَقَ","numberInSurah":2,"juz":30,"manzil":7,"page":604,"ruku":555,"hizbQuarter":240,"sajda":false},{"number":6228,"text":"وَمِن شَرِّ غَاسِقٍ إِذَا وَقَبَ","numberInSurah":3,"juz":30,"manzil":7,"page":604,"ruku":555,"hizbQuarter":240,"sajda":false},{"number":6229,"text":"وَمِن شَرِّ ٱلنَّفَّٰثَٰتِ فِى ٱلْعُقَدِ","numberInSurah":4,"juz":30,"manzil":7,"page":604,"ruku":555,"hizbQuarter":240,"sajda":false},{"number":6230,"text":"وَمِن شَرِّ حَاسِدٍ إِذَا حَسَدَ","numberInSurah":5,"juz":30,"manzil":7,"page":604,"ruku":555,"hizbQuarter":240,"sajda":false}],"edition":{"identifier":"quran-uthmani","language":"ar","name":"Uthamani","englishName":"Uthamani","format":"text","type":"quran"}},{"number":113,"name":"سورة الفلق","englishName":"Al-Falaq","englishNameTranslation":"The Dawn","revelationType":"Meccan","numberOfAyahs":5,"ayahs":[{"number":6226,"text":"Katakanlah: \"Aku berlindung kepada Tuhan Yang Menguasai subuh,","numberInSurah":1,"juz":30,"manzil":7,"page":604,"ruku":555,"hizbQuarter":240,"sajda":false},{"number":6227,"text":"dari kejahatan makhluk-Nya,","numberInSurah":2,"juz":30,"manzil":7,"page":604,"ruku":555,"hizbQuarter":240,"sajda":false},{"number":6228,"text":"dan dari kejahatan malam apabila telah gelap gulita,","numberInSurah":3,"juz":30,"manzil":7,"page":604,"ruku":555,"hizbQuarter":240,"sajda":false},{"number":6229,"text":"dan dari kejahatan wanita-wanita tukang sihir yang menghembus pada buhul-buhul,","numberInSurah":4,"juz":30,"manzil":7,"page":604,"ruku":555,"hizbQuarter":240,"sajda":false},{"number":6230,"text":"dan dari kejahatan pendengki bila ia dengki\".","numberInSurah":5,"juz":30,"manzil":7,"page":604,"ruku":555,"hizbQuarter":240,"sajda":false}],"edition":{"identifier":"id.indonesian","language":"id","name":"Bahasa Indonesia","englishName":"Unknown","format":"text","type":"translation"}},{"number":113,"name":"سورة الفلق","englishName":"Al-Falaq","englishNameTranslation":"The Dawn","revelationType":"Meccan","numberOfAyahs":5,"ayahs":[{"number":6226,"text":"Say, \"I seek refuge in the Lord of daybreak","numberInSurah":1,"juz":30,"manzil":7,"page":604,"ruku":555,"hizbQuarter":240,"sajda":false},{"number":6227,"text":"From the evil of that which He created","numberInSurah":2,"juz":30,"manzil":7,"page":604,"ruku":555,"hizbQuarter":240,"sajda":false},{"number":6228,"text":"And from the evil of darkness when it settles","numberInSurah":3,"juz":30,"manzil":7,"page":604,"ruku":555,"hizbQuarter":240,"sajda":false},{"number":6229,"text":"And from the evil of the blowers in knots","numberInSurah":4,"juz":30,"manzil":7,"page":604,"ruku":555,"hizbQuarter":240,"sajda":false},{"number":6230,"text":"And from the evil of an envier when he envies.\"","numberInSurah":5,"juz":30,"manzil":7,"page":604,"ruku":555,"hizbQuarter":240,"sajda":false}],"edition":{"identifier":"en.sahih","language":"en","name":"Saheeh International","englishName":"Saheeh International","format":"text","type":"translation"}}]}