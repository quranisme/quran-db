umberInSurah":30,"juz":29,"manzil":7,"page":578,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5582,"text":"فَلَا صَدَّقَ وَلَا صَلَّىٰ","numberInSurah":31,"juz":29,"manzil":7,"page":578,"ruku":513,"hizbQuarter":231,"sajda":false},{"number":5583,"text":"وَلَٰكِن كَذَّبَ وَتَوَلَّىٰ","numberInSurah":32,"juz":29,"manzil":7,"page":578,"ruku":513,"hizbQuarter":231,"sajda":false},{"number":5584,"text":"ثُمَّ ذَهَبَ إِلَىٰٓ أَهْلِهِۦ يَتَمَطَّىٰٓ","numberInSurah":33,"juz":29,"manzil":7,"page":578,"ruku":513,"hizbQuarter":231,"sajda":false},{"number":5585,"text":"أَوْلَىٰ لَكَ فَأَوْلَىٰ","numberInSurah":34,"juz":29,"manzil":7,"page":578,"ruku":513,"hizbQuarter":231,"sajda":false},{"number":5586,"text":"ثُمَّ أَوْلَىٰ لَكَ فَأَوْلَىٰٓ","numberInSurah":35,"juz":29,"manzil":7,"page":578,"ruku":513,"hizbQuarter":231,"sajda":false},{"number":5587,"text":"أَيَحْسَبُ ٱلْإِنسَٰنُ أَن يُتْرَكَ سُدًى","numberInSurah":36,"juz":29,"manzil":7,"page":578,"ruku":513,"hizbQuarter":231,"sajda":false},{"number":5588,"text":"أَلَمْ يَكُ نُطْفَةًۭ مِّن مَّنِىٍّۢ يُمْنَىٰ","numberInSurah":37,"juz":29,"manzil":7,"page":578,"ruku":513,"hizbQuarter":231,"sajda":false},{"number":5589,"text":"ثُمَّ كَانَ عَلَقَةًۭ فَخَلَقَ فَسَوَّىٰ","numberInSurah":38,"juz":29,"manzil":7,"page":578,"ruku":513,"hizbQuarter":231,"sajda":false},{"number":5590,"text":"فَجَعَلَ مِنْهُ ٱلزَّوْجَيْنِ ٱلذَّكَرَ وَٱلْأُنثَىٰٓ","numberInSurah":39,"juz":29,"manzil":7,"page":578,"ruku":513,"hizbQuarter":231,"sajda":false},{"number":5591,"text":"أَلَيْسَ ذَٰلِكَ بِقَٰدِرٍ عَلَىٰٓ أَن يُحْۦِىَ ٱلْمَوْتَىٰ","numberInSurah":40,"juz":29,"manzil":7,"page":578,"ruku":513,"hizbQuarter":231,"sajda":false}],"edition":{"identifier":"quran-uthmani","language":"ar","name":"Uthamani","englishName":"Uthamani","format":"text","type":"quran"}},{"number":75,"name":"سورة القيامة","englishName":"Al-Qiyaama","englishNameTranslation":"The Resurrection","revelationType":"Meccan","numberOfAyahs":40,"ayahs":[{"number":5552,"text":"Aku bersumpah demi hari kiamat,","numberInSurah":1,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5553,"text":"dan aku bersumpah dengan jiwa yang amat menyesali (dirinya sendiri).","numberInSurah":2,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5554,"text":"Apakah manusia mengira, bahwa Kami tidak akan mengumpulkan (kembali) tulang belulangnya?","numberInSurah":3,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5555,"text":"Bukan demikian, sebenarnya Kami kuasa menyusun (kembali) jari jemarinya dengan sempurna.","numberInSurah":4,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5556,"text":"Bahkan manusia itu hendak membuat maksiat terus menerus.","numberInSurah":5,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5557,"text":"Ia berkata: \"Bilakah hari kiamat itu?\"","numberInSurah":6,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5558,"text":"Maka apabila mata terbelalak (ketakutan),","numberInSurah":7,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5559,"text":"dan apabila bulan telah hilang cahayanya,","numberInSurah":8,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5560,"text":"dan matahari dan bulan dikumpulkan,","numberInSurah":9,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5561,"text":"pada hari itu manusia berkata: \"Ke mana tempat berlari?\"","numberInSurah":10,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5562,"text":"sekali-kali tidak! Tidak ada tempat berlindung!","numberInSurah":11,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5563,"text":"Hanya kepada Tuhanmu sajalah pada hari itu tempat kembali.","numberInSurah":12,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5564,"text":"Pada hari itu diberitakan kepada manusia apa yang telah dikerjakannya dan apa yang dilalaikannya.","numberInSurah":13,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5565,"text":"Bahkan manusia itu menjadi saksi atas dirinya sendiri,","numberInSurah":14,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5566,"text":"meskipun dia mengemukakan alasan-alasannya.","numberInSurah":15,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5567,"text":"Janganlah kamu gerakkan lidahmu untuk (membaca) Al Quran karena hendak cepat-cepat (menguasai)nya.","numberInSurah":16,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5568,"text":"Sesungguhnya atas tanggungan Kamilah mengumpulkannya (di dadamu) dan (membuatmu pandai) membacanya.","numberInSurah":17,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5569,"text":"Apabila Kami telah selesai membacakannya maka ikutilah bacaannya itu.","numberInSurah":18,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5570,"text":"Kemudian, sesungguhnya atas tanggungan Kamilah penjelasannya.","numberInSurah":19,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5571,"text":"Sekali-kali janganlah demikian. Sebenarnya kamu (hai manusia) mencintai kehidupan dunia,","numberInSurah":20,"juz":29,"manzil":7,"page":578,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5572,"text":"dan meninggalkan (kehidupan) akhirat.","numberInSurah":21,"juz":29,"manzil":7,"page":578,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5573,"text":"Wajah-wajah (orang-orang mukmin) pada hari itu berseri-seri.","numberInSurah":22,"juz":29,"manzil":7,"page":578,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5574,"text":"Kepada Tuhannyalah mereka melihat.","numberInSurah":23,"juz":29,"manzil":7,"page":578,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5575,"text":"Dan wajah-wajah (orang kafir) pada hari itu muram,","numberInSurah":24,"juz":29,"manzil":7,"page":578,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5576,"text":"mereka yakin bahwa akan ditimpakan kepadanya malapetaka yang amat dahsyat.","numberInSurah":25,"juz":29,"manzil":7,"page":578,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5577,"text":"Sekali-kali jangan. Apabila nafas (seseorang) telah (mendesak) sampai ke kerongkongan,","numberInSurah":26,"juz":29,"manzil":7,"page":578,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5578,"text":"dan dikatakan (kepadanya): \"Siapakah yang dapat menyembuhkan?\",","numberInSurah":27,"juz":29,"manzil":7,"page":578,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5579,"text":"dan dia yakin bahwa sesungguhnya itulah waktu perpisahan (dengan dunia),","numberInSurah":28,"juz":29,"manzil":7,"page":578,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5580,"text":"dan bertaut betis (kiri) dan betis (kanan),","numberInSurah":29,"juz":29,"manzil":7,"page":578,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5581,"text":"kepada Tuhanmulah pada hari itu kamu dihalau.","numberInSurah":30,"juz":29,"manzil":7,"page":578,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5582,"text":"Dan ia tidak mau membenarkan (Rasul dan Al Quran) dan tidak mau mengerjakan shalat,","numberInSurah":31,"juz":29,"manzil":7,"page":578,"ruku":513,"hizbQuarter":231,"sajda":false},{"number":5583,"text":"tetapi ia mendustakan (Rasul) dam berpaling (dari kebenaran),","numberInSurah":32,"juz":29,"manzil":7,"page":578,"ruku":513,"hizbQuarter":231,"sajda":false},{"number":5584,"text":"kemudian ia pergi kepada ahlinya dengan berlagak (sombong).","numberInSurah":33,"juz":29,"manzil":7,"page":578,"ruku":513,"hizbQuarter":231,"sajda":false},{"number":5585,"text":"Kecelakaanlah bagimu (hai orang kafir) dan kecelakaanlah bagimu,","numberInSurah":34,"juz":29,"manzil":7,"page":578,"ruku":513,"hizbQuarter":231,"sajda":false},{"number":5586,"text":"kemudian kecelakaanlah bagimu (hai orang kafir) dan kecelakaanlah bagimu.","numberInSurah":35,"juz":29,"manzil":7,"page":578,"ruku":513,"hizbQuarter":231,"sajda":false},{"number":5587,"text":"Apakah manusia mengira, bahwa ia akan dibiarkan begitu saja (tanpa pertanggung jawaban)?","numberInSurah":36,"juz":29,"manzil":7,"page":578,"ruku":513,"hizbQuarter":231,"sajda":false},{"number":5588,"text":"Bukankah dia dahulu setetes mani yang ditumpahkan (ke dalam rahim),","numberInSurah":37,"juz":29,"manzil":7,"page":578,"ruku":513,"hizbQuarter":231,"sajda":false},{"number":5589,"text":"kemudian mani itu menjadi segumpal darah, lalu Allah menciptakannya, dan menyempurnakannya,","numberInSurah":38,"juz":29,"manzil":7,"page":578,"ruku":513,"hizbQuarter":231,"sajda":false},{"number":5590,"text":"lalu Allah menjadikan daripadanya sepasang: laki-laki dan perempuan.","numberInSurah":39,"juz":29,"manzil":7,"page":578,"ruku":513,"hizbQuarter":231,"sajda":false},{"number":5591,"text":"Bukankah (Allah yang berbuat) demikian berkuasa (pula) menghidupkan orang mati?","numberInSurah":40,"juz":29,"manzil":7,"page":578,"ruku":513,"hizbQuarter":231,"sajda":false}],"edition":{"identifier":"id.indonesian","language":"id","name":"Bahasa Indonesia","englishName":"Unknown","format":"text","type":"translation"}},{"number":75,"name":"سورة القيامة","englishName":"Al-Qiyaama","englishNameTranslation":"The Resurrection","revelationType":"Meccan","numberOfAyahs":40,"ayahs":[{"number":5552,"text":"I swear by the Day of Resurrection","numberInSurah":1,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5553,"text":"And I swear by the reproaching soul [to the certainty of resurrection].","numberInSurah":2,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5554,"text":"Does man think that We will not assemble his bones?","numberInSurah":3,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5555,"text":"Yes. [We are] Able [even] to proportion his fingertips.","numberInSurah":4,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5556,"text":"But man desires to continue in sin.","numberInSurah":5,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5557,"text":"He asks, \"When is the Day of Resurrection?\"","numberInSurah":6,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5558,"text":"So when vision is dazzled","numberInSurah":7,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5559,"text":"And the moon darkens","numberInSurah":8,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5560,"text":"And the sun and the moon are joined,","numberInSurah":9,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5561,"text":"Man will say on that Day, \"Where is the [place of] escape?\"","numberInSurah":10,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5562,"text":"No! There is no refuge.","numberInSurah":11,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5563,"text":"To your Lord, that Day, is the [place of] permanence.","numberInSurah":12,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5564,"text":"Man will be informed that Day of what he sent ahead and kept back.","numberInSurah":13,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5565,"text":"Rather, man, against himself, will be a witness,","numberInSurah":14,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5566,"text":"Even if he presents his excuses.","numberInSurah":15,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5567,"text":"Move not your tongue with it, [O Muhammad], to hasten with recitation of the Qur'an.","numberInSurah":16,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5568,"text":"Indeed, upon Us is its collection [in your heart] and [to make possible] its recitation.","numberInSurah":17,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5569,"text":"So when We have recited it [through Gabriel], then follow its recitation.","numberInSurah":18,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5570,"text":"Then upon Us is its clarification [to you].","numberInSurah":19,"juz":29,"manzil":7,"page":577,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5571,"text":"No! But you love the immediate","numberInSurah":20,"juz":29,"manzil":7,"page":578,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5572,"text":"And leave the Hereafter.","numberInSurah":21,"juz":29,"manzil":7,"page":578,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5573,"text":"[Some] faces, that Day, will be radiant,","numberInSurah":22,"juz":29,"manzil":7,"page":578,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5574,"text":"Looking at their Lord.","numberInSurah":23,"juz":29,"manzil":7,"page":578,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5575,"text":"And [some] faces, that Day, will be contorted,","numberInSurah":24,"juz":29,"manzil":7,"page":578,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5576,"text":"Expecting that there will be done to them [something] backbreaking.","numberInSurah":25,"juz":29,"manzil":7,"page":578,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5577,"text":"No! When the soul has reached the collar bones","numberInSurah":26,"juz":29,"manzil":7,"page":578,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5578,"text":"And it is said, \"Who will cure [him]?\"","numberInSurah":27,"juz":29,"manzil":7,"page":578,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5579,"text":"And the dying one is certain that it is the [time of] separation","numberInSurah":28,"juz":29,"manzil":7,"page":578,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5580,"text":"And the leg is wound about the leg,","numberInSurah":29,"juz":29,"manzil":7,"page":578,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5581,"text":"To your Lord, that Day, will be the procession.","numberInSurah":30,"juz":29,"manzil":7,"page":578,"ruku":512,"hizbQuarter":231,"sajda":false},{"number":5582,"text":"And the disbeliever had not believed, nor had he prayed.","numberInSurah":31,"juz":29,"manzil":7,"page":578,"ruku":513,"hizbQuarter":231,"sajda":false},{"number":5583,"text":"But [instead], he denied and turned away.","numberInSurah":32,"juz":29,"manzil":7,"page":578,"ruku":513,"hizbQuarter":231,"sajda":false},{"number":5584,"text":"And then he went to his people, swaggering [in pride].","numberInSurah":33,"juz":29,"manzil":7,"page":578,"ruku":513,"hizbQuarter":231,"sajda":false},{"number":5585,"text":"Woe to you, and woe!","numberInSurah":34,"juz":29,"manzil":7,"page":578,"ruku":513,"hizbQuarter":231,"sajda":false},{"number":5586,"text":"Then woe to you, and woe!","numberInSurah":35,"juz":29,"manzil":7,"page":578,"ruku":513,"hizbQuarter":231,"sajda":false},{"number":5587,"text":"Does man think that he will be left neglected?","numberInSurah":36,"juz":29,"manzil":7,"page":578,"ruku":513,"hizbQuarter":231,"sajda":false},{"number":5588,"text":"Had he not been a sperm from semen emitted?","numberInSurah":37,"juz":29,"manzil":7,"page":578,"ruku":513,"hizbQuarter":231,"sajda":false},{"number":5589,"text":"Then he was a clinging clot, and [Allah] created [his form] and proportioned [him]","numberInSurah":38,"juz":29,"manzil":7,"page":578,"ruku":513,"hizbQuarter":231,"sajda":false},{"number":5590,"text":"And made of him two mates, the male and the female.","numberInSurah":39,"juz":29,"manzil":7,"page":578,"ruku":513,"hizbQuarter":231,"sajda":false},{"number":5591,"text":"Is not that [Creator] Able to give life to the dead?","numberInSurah":40,"juz":29,"manzil":7,"page":578,"ruku":513,"hizbQuarter":231,"sajda":false}],"edition":{"identifier":"en.sahih","language":"en","name":"Saheeh International","englishName":"Saheeh International","format":"text","type":"translation"}}]}