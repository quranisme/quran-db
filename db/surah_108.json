{"code":200,"status":"OK","data":[{"number":108,"name":"سورة الكوثر","englishName":"Al-Kawthar","englishNameTranslation":"Abundance","revelationType":"Meccan","numberOfAyahs":3,"ayahs":[{"number":6205,"audio":"http://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6205","text":"بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ إِنَّا أَعْطَيْنَاكَ الْكَوْثَرَ","numberInSurah":1,"juz":30,"manzil":7,"page":602,"ruku":550,"hizbQuarter":240,"sajda":false},{"number":6206,"audio":"http://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6206","text":"فَصَلِّ لِرَبِّكَ وَانْحَرْ","numberInSurah":2,"juz":30,"manzil":7,"page":602,"ruku":550,"hizbQuarter":240,"sajda":false},{"number":6207,"audio":"http://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6207","text":"إِنَّ شَانِئَكَ هُوَ الْأَبْتَرُ","numberInSurah":3,"juz":30,"manzil":7,"page":602,"ruku":550,"hizbQuarter":240,"sajda":false}],"edition":{"identifier":"ar.alafasy","language":"ar","name":"Alafasy","englishName":"Alafasy","format":"audio","type":"versebyverse"}},{"number":108,"name":"سورة الكوثر","englishName":"Al-Kawthar","englishNameTranslation":"Abundance","revelationType":"Meccan","numberOfAyahs":3,"ayahs":[{"number":6205,"text":"بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ إِنَّآ أَعْطَيْنَٰكَ ٱلْكَوْثَرَ","numberInSurah":1,"juz":30,"manzil":7,"page":602,"ruku":550,"hizbQuarter":240,"sajda":false},{"number":6206,"text":"فَصَلِّ لِرَبِّكَ وَٱنْحَرْ","numberInSurah":2,"juz":30,"manzil":7,"page":602,"ruku":550,"hizbQuarter":240,"sajda":false},{"number":6207,"text":"إِنَّ شَانِئَكَ هُوَ ٱلْأَبْتَرُ","numberInSurah":3,"juz":30,"manzil":7,"page":602,"ruku":550,"hizbQuarter":240,"sajda":false}],"edition":{"identifier":"quran-uthmani","language":"ar","name":"Uthamani","englishName":"Uthamani","format":"text","type":"quran"}},{"number":108,"name":"سورة الكوثر","englishName":"Al-Kawthar","englishNameTranslation":"Abundance","revelationType":"Meccan","numberOfAyahs":3,"ayahs":[{"number":6205,"text":"Sesungguhnya Kami telah memberikan kepadamu nikmat yang banyak.","numberInSurah":1,"juz":30,"manzil":7,"page":602,"ruku":550,"hizbQuarter":240,"sajda":false},{"number":6206,"text":"Maka dirikanlah shalat karena Tuhanmu; dan berkorbanlah.","numberInSurah":2,"juz":30,"manzil":7,"page":602,"ruku":550,"hizbQuarter":240,"sajda":false},{"number":6207,"text":"Sesungguhnya orang-orang yang membenci kamu dialah yang terputus.","numberInSurah":3,"juz":30,"manzil":7,"page":602,"ruku":550,"hizbQuarter":240,"sajda":false}],"edition":{"identifier":"id.indonesian","language":"id","name":"Bahasa Indonesia","englishName":"Unknown","format":"text","type":"translation"}},{"number":108,"name":"سورة الكوثر","englishName":"Al-Kawthar","englishNameTranslation":"Abundance","revelationType":"Meccan","numberOfAyahs":3,"ayahs":[{"number":6205,"text":"Indeed, We have granted you, [O Muhammad], al-Kawthar.","numberInSurah":1,"juz":30,"manzil":7,"page":602,"ruku":550,"hizbQuarter":240,"sajda":false},{"number":6206,"text":"So pray to your Lord and sacrifice [to Him alone].","numberInSurah":2,"juz":30,"manzil":7,"page":602,"ruku":550,"hizbQuarter":240,"sajda":false},{"number":6207,"text":"Indeed, your enemy is the one cut off.","numberInSurah":3,"juz":30,"manzil":7,"page":602,"ruku":550,"hizbQuarter":240,"sajda":false}],"edition":{"identifier":"en.sahih","language":"en","name":"Saheeh International","englishName":"Saheeh International","format":"text","type":"translation"}}]}