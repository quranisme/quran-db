    {
      "number": 1,
      "name": "سورة الفاتحة",
      "englishName": "Al-Faatiha",
      "englishNameTranslation": "The Opening",
      "revelationType": "Meccan",
      "numberOfAyahs": 7,
      "ayahs": [
        {
          "number": 1,
          "text": "Dengan menyebut nama Allah Yang Maha Pemurah lagi Maha Penyayang.",
          "numberInSurah": 1,
          "juz": 1,
          "manzil": 1,
          "page": 1,
          "ruku": 1,
          "hizbQuarter": 1,
          "sajda": false
        },
        {
          "number": 2,
          "text": "Segala puji bagi Allah, Tuhan semesta alam.",
          "numberInSurah": 2,
          "juz": 1,
          "manzil": 1,
          "page": 1,
          "ruku": 1,
          "hizbQuarter": 1,
          "sajda": false
        },
        {
          "number": 3,
          "text": "Maha Pemurah lagi Maha Penyayang.",
          "numberInSurah": 3,
          "juz": 1,
          "manzil": 1,
          "page": 1,
          "ruku": 1,
          "hizbQuarter": 1,
          "sajda": false
        },
        {
          "number": 4,
          "text": "Yang menguasai di Hari Pembalasan.",
          "numberInSurah": 4,
          "juz": 1,
          "manzil": 1,
          "page": 1,
          "ruku": 1,
          "hizbQuarter": 1,
          "sajda": false
        },
        {
          "number": 5,
          "text": "Hanya Engkaulah yang kami sembah, dan hanya kepada Engkaulah kami meminta pertolongan.",
          "numberInSurah": 5,
          "juz": 1,
          "manzil": 1,
          "page": 1,
          "ruku": 1,
          "hizbQuarter": 1,
          "sajda": false
        },
        {
          "number": 6,
          "text": "Tunjukilah kami jalan yang lurus,",
          "numberInSurah": 6,
          "juz": 1,
          "manzil": 1,
          "page": 1,
          "ruku": 1,
          "hizbQuarter": 1,
          "sajda": false
        },
        {
          "number": 7,
          "text": "(yaitu) Jalan orang-orang yang telah Engkau beri nikmat kepada mereka; bukan (jalan) mereka yang dimurkai dan bukan (pula jalan) mereka yang sesat.",
          "numberInSurah": 7,
          "juz": 1,
          "manzil": 1,
          "page": 1,
          "ruku": 1,
          "hizbQuarter": 1,
          "sajda": false
        }
      ],
      "edition": {
        "identifier": "id.indonesian",
        "language": "id",
        "name": "Bahasa Indonesia",
