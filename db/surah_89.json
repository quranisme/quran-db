ter":237,"sajda":false},{"number":6011,"text":"وَلَا تَحَٰٓضُّونَ عَلَىٰ طَعَامِ ٱلْمِسْكِينِ","numberInSurah":18,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6012,"text":"وَتَأْكُلُونَ ٱلتُّرَاثَ أَكْلًۭا لَّمًّۭا","numberInSurah":19,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6013,"text":"وَتُحِبُّونَ ٱلْمَالَ حُبًّۭا جَمًّۭا","numberInSurah":20,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6014,"text":"كَلَّآ إِذَا دُكَّتِ ٱلْأَرْضُ دَكًّۭا دَكًّۭا","numberInSurah":21,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6015,"text":"وَجَآءَ رَبُّكَ وَٱلْمَلَكُ صَفًّۭا صَفًّۭا","numberInSurah":22,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6016,"text":"وَجِا۟ىٓءَ يَوْمَئِذٍۭ بِجَهَنَّمَ ۚ يَوْمَئِذٍۢ يَتَذَكَّرُ ٱلْإِنسَٰنُ وَأَنَّىٰ لَهُ ٱلذِّكْرَىٰ","numberInSurah":23,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6017,"text":"يَقُولُ يَٰلَيْتَنِى قَدَّمْتُ لِحَيَاتِى","numberInSurah":24,"juz":30,"manzil":7,"page":594,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6018,"text":"فَيَوْمَئِذٍۢ لَّا يُعَذِّبُ عَذَابَهُۥٓ أَحَدٌۭ","numberInSurah":25,"juz":30,"manzil":7,"page":594,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6019,"text":"وَلَا يُوثِقُ وَثَاقَهُۥٓ أَحَدٌۭ","numberInSurah":26,"juz":30,"manzil":7,"page":594,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6020,"text":"يَٰٓأَيَّتُهَا ٱلنَّفْسُ ٱلْمُطْمَئِنَّةُ","numberInSurah":27,"juz":30,"manzil":7,"page":594,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6021,"text":"ٱرْجِعِىٓ إِلَىٰ رَبِّكِ رَاضِيَةًۭ مَّرْضِيَّةًۭ","numberInSurah":28,"juz":30,"manzil":7,"page":594,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6022,"text":"فَٱدْخُلِى فِى عِبَٰدِى","numberInSurah":29,"juz":30,"manzil":7,"page":594,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6023,"text":"وَٱدْخُلِى جَنَّتِى","numberInSurah":30,"juz":30,"manzil":7,"page":594,"ruku":531,"hizbQuarter":237,"sajda":false}],"edition":{"identifier":"quran-uthmani","language":"ar","name":"Uthamani","englishName":"Uthamani","format":"text","type":"quran"}},{"number":89,"name":"سورة الفجر","englishName":"Al-Fajr","englishNameTranslation":"The Dawn","revelationType":"Meccan","numberOfAyahs":30,"ayahs":[{"number":5994,"text":"Demi fajar,","numberInSurah":1,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":5995,"text":"dan malam yang sepuluh,","numberInSurah":2,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":5996,"text":"dan yang genap dan yang ganjil,","numberInSurah":3,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":5997,"text":"dan malam bila berlalu.","numberInSurah":4,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":5998,"text":"Pada yang demikian itu terdapat sumpah (yang dapat diterima) oleh orang-orang yang berakal.","numberInSurah":5,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":5999,"text":"Apakah kamu tidak memperhatikan bagaimana Tuhanmu berbuat terhadap kaum 'Aad?","numberInSurah":6,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6000,"text":"(yaitu) penduduk Iram yang mempunyai bangunan-bangunan yang tinggi,","numberInSurah":7,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6001,"text":"yang belum pernah dibangun (suatu kota) seperti itu, di negeri-negeri lain,","numberInSurah":8,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6002,"text":"dan kaum Tsamud yang memotong batu-batu besar di lembah,","numberInSurah":9,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6003,"text":"dan kaum Fir'aun yang mempunyai pasak-pasak (tentara yang banyak),","numberInSurah":10,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6004,"text":"yang berbuat sewenang-wenang dalam negeri,","numberInSurah":11,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6005,"text":"lalu mereka berbuat banyak kerusakan dalam negeri itu,","numberInSurah":12,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6006,"text":"karena itu Tuhanmu menimpakan kepada mereka cemeti azab,","numberInSurah":13,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6007,"text":"sesungguhnya Tuhanmu benar-benar mengawasi.","numberInSurah":14,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6008,"text":"Adapun manusia apabila Tuhannya mengujinya lalu dia dimuliakan-Nya dan diberi-Nya kesenangan, maka dia akan berkata: \"Tuhanku telah memuliakanku\".","numberInSurah":15,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6009,"text":"Adapun bila Tuhannya mengujinya lalu membatasi rezekinya maka dia berkata: \"Tuhanku menghinakanku\".","numberInSurah":16,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6010,"text":"Sekali-kali tidak (demikian), sebenarnya kamu tidak memuliakan anak yatim,","numberInSurah":17,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6011,"text":"dan kamu tidak saling mengajak memberi makan orang miskin,","numberInSurah":18,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6012,"text":"dan kamu memakan harta pusaka dengan cara mencampur baurkan (yang halal dan yang bathil),","numberInSurah":19,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6013,"text":"dan kamu mencintai harta benda dengan kecintaan yang berlebihan.","numberInSurah":20,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6014,"text":"Jangan (berbuat demikian). Apabila bumi digoncangkan berturut-turut,","numberInSurah":21,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6015,"text":"dan datanglah Tuhanmu; sedang malaikat berbaris-baris.","numberInSurah":22,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6016,"text":"Dan pada hari itu diperlihatkan neraka Jahannam; dan pada hari itu ingatlah manusia, akan tetapi tidak berguna lagi mengingat itu baginya.","numberInSurah":23,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6017,"text":"Dia mengatakan: \"Alangkah baiknya kiranya aku dahulu mengerjakan (amal saleh) untuk hidupku ini\".","numberInSurah":24,"juz":30,"manzil":7,"page":594,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6018,"text":"Maka pada hari itu tiada seorangpun yang menyiksa seperti siksa-Nya.","numberInSurah":25,"juz":30,"manzil":7,"page":594,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6019,"text":"dan tiada seorangpun yang mengikat seperti ikatan-Nya.","numberInSurah":26,"juz":30,"manzil":7,"page":594,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6020,"text":"Hai jiwa yang tenang.","numberInSurah":27,"juz":30,"manzil":7,"page":594,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6021,"text":"Kembalilah kepada Tuhanmu dengan hati yang puas lagi diridhai-Nya.","numberInSurah":28,"juz":30,"manzil":7,"page":594,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6022,"text":"Maka masuklah ke dalam jama'ah hamba-hamba-Ku,","numberInSurah":29,"juz":30,"manzil":7,"page":594,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6023,"text":"masuklah ke dalam surga-Ku.","numberInSurah":30,"juz":30,"manzil":7,"page":594,"ruku":531,"hizbQuarter":237,"sajda":false}],"edition":{"identifier":"id.indonesian","language":"id","name":"Bahasa Indonesia","englishName":"Unknown","format":"text","type":"translation"}},{"number":89,"name":"سورة الفجر","englishName":"Al-Fajr","englishNameTranslation":"The Dawn","revelationType":"Meccan","numberOfAyahs":30,"ayahs":[{"number":5994,"text":"By the dawn","numberInSurah":1,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":5995,"text":"And [by] ten nights","numberInSurah":2,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":5996,"text":"And [by] the even [number] and the odd","numberInSurah":3,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":5997,"text":"And [by] the night when it passes,","numberInSurah":4,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":5998,"text":"Is there [not] in [all] that an oath [sufficient] for one of perception?","numberInSurah":5,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":5999,"text":"Have you not considered how your Lord dealt with 'Aad -","numberInSurah":6,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6000,"text":"[With] Iram - who had lofty pillars,","numberInSurah":7,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6001,"text":"The likes of whom had never been created in the land?","numberInSurah":8,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6002,"text":"And [with] Thamud, who carved out the rocks in the valley?","numberInSurah":9,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6003,"text":"And [with] Pharaoh, owner of the stakes? -","numberInSurah":10,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6004,"text":"[All of] whom oppressed within the lands","numberInSurah":11,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6005,"text":"And increased therein the corruption.","numberInSurah":12,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6006,"text":"So your Lord poured upon them a scourge of punishment.","numberInSurah":13,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6007,"text":"Indeed, your Lord is in observation.","numberInSurah":14,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6008,"text":"And as for man, when his Lord tries him and [thus] is generous to him and favors him, he says, \"My Lord has honored me.\"","numberInSurah":15,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6009,"text":"But when He tries him and restricts his provision, he says, \"My Lord has humiliated me.\"","numberInSurah":16,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6010,"text":"No! But you do not honor the orphan","numberInSurah":17,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6011,"text":"And you do not encourage one another to feed the poor.","numberInSurah":18,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6012,"text":"And you consume inheritance, devouring [it] altogether,","numberInSurah":19,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6013,"text":"And you love wealth with immense love.","numberInSurah":20,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6014,"text":"No! When the earth has been leveled - pounded and crushed -","numberInSurah":21,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6015,"text":"And your Lord has come and the angels, rank upon rank,","numberInSurah":22,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6016,"text":"And brought [within view], that Day, is Hell - that Day, man will remember, but what good to him will be the remembrance?","numberInSurah":23,"juz":30,"manzil":7,"page":593,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6017,"text":"He will say, \"Oh, I wish I had sent ahead [some good] for my life.\"","numberInSurah":24,"juz":30,"manzil":7,"page":594,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6018,"text":"So on that Day, none will punish [as severely] as His punishment,","numberInSurah":25,"juz":30,"manzil":7,"page":594,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6019,"text":"And none will bind [as severely] as His binding [of the evildoers].","numberInSurah":26,"juz":30,"manzil":7,"page":594,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6020,"text":"[To the righteous it will be said], \"O reassured soul,","numberInSurah":27,"juz":30,"manzil":7,"page":594,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6021,"text":"Return to your Lord, well-pleased and pleasing [to Him],","numberInSurah":28,"juz":30,"manzil":7,"page":594,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6022,"text":"And enter among My [righteous] servants","numberInSurah":29,"juz":30,"manzil":7,"page":594,"ruku":531,"hizbQuarter":237,"sajda":false},{"number":6023,"text":"And enter My Paradise.\"","numberInSurah":30,"juz":30,"manzil":7,"page":594,"ruku":531,"hizbQuarter":237,"sajda":false}],"edition":{"identifier":"en.sahih","language":"en","name":"Saheeh International","englishName":"Saheeh International","format":"text","type":"translation"}}]}