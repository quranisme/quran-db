{"code":200,"status":"OK","data":[{"number":107,"name":"سورة الماعون","englishName":"Al-Maa'un","englishNameTranslation":"Almsgiving","revelationType":"Meccan","numberOfAyahs":7,"ayahs":[{"number":6198,"audio":"http://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6198","text":"بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ أَرَأَيْتَ الَّذِي يُكَذِّبُ بِالدِّينِ","numberInSurah":1,"juz":30,"manzil":7,"page":602,"ruku":549,"hizbQuarter":240,"sajda":false},{"number":6199,"audio":"http://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6199","text":"فَذَٰلِكَ الَّذِي يَدُعُّ الْيَتِيمَ","numberInSurah":2,"juz":30,"manzil":7,"page":602,"ruku":549,"hizbQuarter":240,"sajda":false},{"number":6200,"audio":"http://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6200","text":"وَلَا يَحُضُّ عَلَىٰ طَعَامِ الْمِسْكِينِ","numberInSurah":3,"juz":30,"manzil":7,"page":602,"ruku":549,"hizbQuarter":240,"sajda":false},{"number":6201,"audio":"http://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6201","text":"فَوَيْلٌ لِلْمُصَلِّينَ","numberInSurah":4,"juz":30,"manzil":7,"page":602,"ruku":549,"hizbQuarter":240,"sajda":false},{"number":6202,"audio":"http://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6202","text":"الَّذِينَ هُمْ عَنْ صَلَاتِهِمْ سَاهُونَ","numberInSurah":5,"juz":30,"manzil":7,"page":602,"ruku":549,"hizbQuarter":240,"sajda":false},{"number":6203,"audio":"http://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6203","text":"الَّذِينَ هُمْ يُرَاءُونَ","numberInSurah":6,"juz":30,"manzil":7,"page":602,"ruku":549,"hizbQuarter":240,"sajda":false},{"number":6204,"audio":"http://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6204","text":"وَيَمْنَعُونَ الْمَاعُونَ","numberInSurah":7,"juz":30,"manzil":7,"page":602,"ruku":549,"hizbQuarter":240,"sajda":false}],"edition":{"identifier":"ar.alafasy","language":"ar","name":"Alafasy","englishName":"Alafasy","format":"audio","type":"versebyverse"}},{"number":107,"name":"سورة الماعون","englishName":"Al-Maa'un","englishNameTranslation":"Almsgiving","revelationType":"Meccan","numberOfAyahs":7,"ayahs":[{"number":6198,"text":"بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ أَرَءَيْتَ ٱلَّذِى يُكَذِّبُ بِٱلدِّينِ","numberInSurah":1,"juz":30,"manzil":7,"page":602,"ruku":549,"hizbQuarter":240,"sajda":false},{"number":6199,"text":"فَذَٰلِكَ ٱلَّذِى يَدُعُّ ٱلْيَتِيمَ","numberInSurah":2,"juz":30,"manzil":7,"page":602,"ruku":549,"hizbQuarter":240,"sajda":false},{"number":6200,"text":"وَلَا يَحُضُّ عَلَىٰ طَعَامِ ٱلْمِسْكِينِ","numberInSurah":3,"juz":30,"manzil":7,"page":602,"ruku":549,"hizbQuarter":240,"sajda":false},{"number":6201,"text":"فَوَيْلٌۭ لِّلْمُصَلِّينَ","numberInSurah":4,"juz":30,"manzil":7,"page":602,"ruku":549,"hizbQuarter":240,"sajda":false},{"number":6202,"text":"ٱلَّذِينَ هُمْ عَن صَلَاتِهِمْ سَاهُونَ","numberInSurah":5,"juz":30,"manzil":7,"page":602,"ruku":549,"hizbQuarter":240,"sajda":false},{"number":6203,"text":"ٱلَّذِينَ هُمْ يُرَآءُونَ","numberInSurah":6,"juz":30,"manzil":7,"page":602,"ruku":549,"hizbQuarter":240,"sajda":false},{"number":6204,"text":"وَيَمْنَعُونَ ٱلْمَاعُونَ","numberInSurah":7,"juz":30,"manzil":7,"page":602,"ruku":549,"hizbQuarter":240,"sajda":false}],"edition":{"identifier":"quran-uthmani","language":"ar","name":"Uthamani","englishName":"Uthamani","format":"text","type":"quran"}},{"number":107,"name":"سورة الماعون","englishName":"Al-Maa'un","englishNameTranslation":"Almsgiving","revelationType":"Meccan","numberOfAyahs":7,"ayahs":[{"number":6198,"text":"Tahukah kamu (orang) yang mendustakan agama?","numberInSurah":1,"juz":30,"manzil":7,"page":602,"ruku":549,"hizbQuarter":240,"sajda":false},{"number":6199,"text":"Itulah orang yang menghardik anak yatim,","numberInSurah":2,"juz":30,"manzil":7,"page":602,"ruku":549,"hizbQuarter":240,"sajda":false},{"number":6200,"text":"dan tidak menganjurkan memberi makan orang miskin.","numberInSurah":3,"juz":30,"manzil":7,"page":602,"ruku":549,"hizbQuarter":240,"sajda":false},{"number":6201,"text":"Maka kecelakaanlah bagi orang-orang yang shalat,","numberInSurah":4,"juz":30,"manzil":7,"page":602,"ruku":549,"hizbQuarter":240,"sajda":false},{"number":6202,"text":"(yaitu) orang-orang yang lalai dari shalatnya,","numberInSurah":5,"juz":30,"manzil":7,"page":602,"ruku":549,"hizbQuarter":240,"sajda":false},{"number":6203,"text":"orang-orang yang berbuat riya,","numberInSurah":6,"juz":30,"manzil":7,"page":602,"ruku":549,"hizbQuarter":240,"sajda":false},{"number":6204,"text":"dan enggan (menolong dengan) barang berguna.","numberInSurah":7,"juz":30,"manzil":7,"page":602,"ruku":549,"hizbQuarter":240,"sajda":false}],"edition":{"identifier":"id.indonesian","language":"id","name":"Bahasa Indonesia","englishName":"Unknown","format":"text","type":"translation"}},{"number":107,"name":"سورة الماعون","englishName":"Al-Maa'un","englishNameTranslation":"Almsgiving","revelationType":"Meccan","numberOfAyahs":7,"ayahs":[{"number":6198,"text":"Have you seen the one who denies the Recompense?","numberInSurah":1,"juz":30,"manzil":7,"page":602,"ruku":549,"hizbQuarter":240,"sajda":false},{"number":6199,"text":"For that is the one who drives away the orphan","numberInSurah":2,"juz":30,"manzil":7,"page":602,"ruku":549,"hizbQuarter":240,"sajda":false},{"number":6200,"text":"And does not encourage the feeding of the poor.","numberInSurah":3,"juz":30,"manzil":7,"page":602,"ruku":549,"hizbQuarter":240,"sajda":false},{"number":6201,"text":"So woe to those who pray","numberInSurah":4,"juz":30,"manzil":7,"page":602,"ruku":549,"hizbQuarter":240,"sajda":false},{"number":6202,"text":"[But] who are heedless of their prayer -","numberInSurah":5,"juz":30,"manzil":7,"page":602,"ruku":549,"hizbQuarter":240,"sajda":false},{"number":6203,"text":"Those who make show [of their deeds]","numberInSurah":6,"juz":30,"manzil":7,"page":602,"ruku":549,"hizbQuarter":240,"sajda":false},{"number":6204,"text":"And withhold [simple] assistance.","numberInSurah":7,"juz":30,"manzil":7,"page":602,"ruku":549,"hizbQuarter":240,"sajda":false}],"edition":{"identifier":"en.sahih","language":"en","name":"Saheeh International","englishName":"Saheeh International","format":"text","type":"translation"}}]}