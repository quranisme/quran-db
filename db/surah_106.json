{"code":200,"status":"OK","data":[{"number":106,"name":"سورة قريش","englishName":"Quraish","englishNameTranslation":"Quraysh","revelationType":"Meccan","numberOfAyahs":4,"ayahs":[{"number":6194,"audio":"http://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6194","text":"بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ لِإِيلَافِ قُرَيْشٍ","numberInSurah":1,"juz":30,"manzil":7,"page":602,"ruku":548,"hizbQuarter":240,"sajda":false},{"number":6195,"audio":"http://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6195","text":"إِيلَافِهِمْ رِحْلَةَ الشِّتَاءِ وَالصَّيْفِ","numberInSurah":2,"juz":30,"manzil":7,"page":602,"ruku":548,"hizbQuarter":240,"sajda":false},{"number":6196,"audio":"http://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6196","text":"فَلْيَعْبُدُوا رَبَّ هَٰذَا الْبَيْتِ","numberInSurah":3,"juz":30,"manzil":7,"page":602,"ruku":548,"hizbQuarter":240,"sajda":false},{"number":6197,"audio":"http://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6197","text":"الَّذِي أَطْعَمَهُمْ مِنْ جُوعٍ وَآمَنَهُمْ مِنْ خَوْفٍ","numberInSurah":4,"juz":30,"manzil":7,"page":602,"ruku":548,"hizbQuarter":240,"sajda":false}],"edition":{"identifier":"ar.alafasy","language":"ar","name":"Alafasy","englishName":"Alafasy","format":"audio","type":"versebyverse"}},{"number":106,"name":"سورة قريش","englishName":"Quraish","englishNameTranslation":"Quraysh","revelationType":"Meccan","numberOfAyahs":4,"ayahs":[{"number":6194,"text":"بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ لِإِيلَٰفِ قُرَيْشٍ","numberInSurah":1,"juz":30,"manzil":7,"page":602,"ruku":548,"hizbQuarter":240,"sajda":false},{"number":6195,"text":"إِۦلَٰفِهِمْ رِحْلَةَ ٱلشِّتَآءِ وَٱلصَّيْفِ","numberInSurah":2,"juz":30,"manzil":7,"page":602,"ruku":548,"hizbQuarter":240,"sajda":false},{"number":6196,"text":"فَلْيَعْبُدُوا۟ رَبَّ هَٰذَا ٱلْبَيْتِ","numberInSurah":3,"juz":30,"manzil":7,"page":602,"ruku":548,"hizbQuarter":240,"sajda":false},{"number":6197,"text":"ٱلَّذِىٓ أَطْعَمَهُم مِّن جُوعٍۢ وَءَامَنَهُم مِّنْ خَوْفٍۭ","numberInSurah":4,"juz":30,"manzil":7,"page":602,"ruku":548,"hizbQuarter":240,"sajda":false}],"edition":{"identifier":"quran-uthmani","language":"ar","name":"Uthamani","englishName":"Uthamani","format":"text","type":"quran"}},{"number":106,"name":"سورة قريش","englishName":"Quraish","englishNameTranslation":"Quraysh","revelationType":"Meccan","numberOfAyahs":4,"ayahs":[{"number":6194,"text":"Karena kebiasaan orang-orang Quraisy,","numberInSurah":1,"juz":30,"manzil":7,"page":602,"ruku":548,"hizbQuarter":240,"sajda":false},{"number":6195,"text":"(yaitu) kebiasaan mereka bepergian pada musim dingin dan musim panas.","numberInSurah":2,"juz":30,"manzil":7,"page":602,"ruku":548,"hizbQuarter":240,"sajda":false},{"number":6196,"text":"Maka hendaklah mereka menyembah Tuhan Pemilik rumah ini (Ka'bah).","numberInSurah":3,"juz":30,"manzil":7,"page":602,"ruku":548,"hizbQuarter":240,"sajda":false},{"number":6197,"text":"Yang telah memberi makanan kepada mereka untuk menghilangkan lapar dan mengamankan mereka dari ketakutan.","numberInSurah":4,"juz":30,"manzil":7,"page":602,"ruku":548,"hizbQuarter":240,"sajda":false}],"edition":{"identifier":"id.indonesian","language":"id","name":"Bahasa Indonesia","englishName":"Unknown","format":"text","type":"translation"}},{"number":106,"name":"سورة قريش","englishName":"Quraish","englishNameTranslation":"Quraysh","revelationType":"Meccan","numberOfAyahs":4,"ayahs":[{"number":6194,"text":"For the accustomed security of the Quraysh -","numberInSurah":1,"juz":30,"manzil":7,"page":602,"ruku":548,"hizbQuarter":240,"sajda":false},{"number":6195,"text":"Their accustomed security [in] the caravan of winter and summer -","numberInSurah":2,"juz":30,"manzil":7,"page":602,"ruku":548,"hizbQuarter":240,"sajda":false},{"number":6196,"text":"Let them worship the Lord of this House,","numberInSurah":3,"juz":30,"manzil":7,"page":602,"ruku":548,"hizbQuarter":240,"sajda":false},{"number":6197,"text":"Who has fed them, [saving them] from hunger and made them safe, [saving them] from fear.","numberInSurah":4,"juz":30,"manzil":7,"page":602,"ruku":548,"hizbQuarter":240,"sajda":false}],"edition":{"identifier":"en.sahih","language":"en","name":"Saheeh International","englishName":"Saheeh International","format":"text","type":"translation"}}]}